{
  "name": "filecoin-pickaxe-direct-deal",
  "version": "0.0.2",
  "description": "Quickly make Filecoin deals for pickaxe bundles",
  "scripts": {
    "dev": "nodemon --ignore dist --exec babel-node index.js",
    "build": "babel *.js --out-dir dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "filecoin-browse-asks": "dist/index.js"
  },
  "keywords": [],
  "author": "Jim Pick (@jimpick)",
  "license": "MIT",
  "dependencies": {
    "@jimpick/filecoin-pickaxe-mineshaft": "^0.0.5",
    "@jimpick/filecoin-pickaxe-mineshaft-context": "^0.0.1",
    "@jimpick/ink-watch-for-exit-key": "^0.0.4",
    "@jimpick/use-filecoin-asks": "^0.0.3",
    "@jimpick/use-filecoin-config": "^0.0.4",
    "@jimpick/use-filecoin-head": "^0.0.4",
    "@jimpick/use-filecoin-network-info": "^0.0.3",
    "bignumber.js": "^8.1.1",
    "figures": "^3.0.0",
    "filecoin-api-client": "^0.2.0",
    "immer": "^3.1.2",
    "ink": "^2.2.0",
    "meow": "^5.0.0",
    "node-fetch": "^2.3.0",
    "pretty-bytes": "^5.2.0",
    "pretty-ms": "^5.0.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "babel-plugin-shebang": "^1.0.0",
    "nodemon": "^1.18.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "shebang",
      "@babel/transform-react-jsx"
    ]
  },
  "browserslist": "node 10",
  "nodemonConfig": {
    "stdin": false,
    "restartable": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimpick/filecoin-pickaxe-direct-deal.git"
  }
}
